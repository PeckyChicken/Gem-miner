{
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#000275",
        "editorCursor.foreground": "#1900ff",
        "minimap.selectionHighlight": "#000275",
    },
    "editor.tokenColorCustomizations": {
        "comments": "#b30000",
        "variables": "#108600",
        "strings": "#00c21a",
        "functions": "#00b7cf",
        "numbers": "#b300b3",
        "keywords": "#7700d8",
        "types":{ 
            "foreground": "#003cff",
            "fontStyle": ""
        },
        "textMateRules":[
            {
                "scope": "keyword.operator.logical",
                "settings": {
                    "foreground": "#dfa300"
                }
            },
            {
                "scope": "keyword.operator.arithmetic",
                "settings": {
                    "foreground": "#b300b3"
                }
            },
            {
                "scope": "keyword.operator.assignment",
                "settings": {
                    "foreground": "#4f5bff"
                }
            },
            {
                "scope": "keyword.operator.assignment",
                "settings": {
                    "foreground": "#4f5bff"
                }
            },
            {
                "scope": "keyword.operator.comparison",
                "settings": {
                    "foreground": "#dfa300"
                }
            }
        ],

    },
    "editor.semanticTokenColorCustomizations": {
        "rules": {
            "module":{
                "foreground": "#00ffaa",
                "fontStyle": ""
            }
        }
      
    }
}